{
  "activationEvents": [
    "onCommand:retrieves.helloWorld"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "retrieves.clone",
        "title": "Clone"
      },
      {
        "command": "retrieves.lines",
        "title": "Attacked Lines"
      }
    ],
    "configuration": [
      {
        "properties": {
          "retrieves.api_token": {
            "type": "string"
          }
        },
        "title": "Retrieves configuration"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "retrieves.clone",
          "when": "view == user_groups && viewItem == gitRoot"
        },
        {
          "command": "retrieves.lines",
          "when": "view == user_groups && viewItem == gitRoot"
        }
      ],
      "editor/title/context": [
        {
          "command": "retrieves.clone",
          "group": "git",
          "when": "config.git.enabled"
        }
      ],
      "explorer/context": [
        {
          "command": "retrieves.clone",
          "group": "git",
          "when": "config.git.enabled && resourcePath =~ **/services/groups/([a-z]*)/fusion/([a-z]*)$/"
        }
      ]
    },
    "views": {
      "groups_explorer": [
        {
          "contextualTitle": "Retrieves",
          "icon": "media/dep.svg",
          "id": "user_groups",
          "name": "User Groups"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "media/dep.svg",
          "id": "groups_explorer",
          "title": "Groups Manager"
        }
      ]
    }
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@types/vscode-webview": "^1.57.1",
    "@vscode/webview-ui-toolkit": "^1.2.1",
    "cross-fetch": "^3.1.5",
    "glob": "^8.1.0",
    "graphql": "^16.6.0",
    "html-creator": "^0.6.2",
    "simple-git": "^3.15.1",
    "tar-stream": "^3.0.0",
    "tmp": "^0.2.1"
  },
  "description": "",
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/tar-stream": "^2.2.2",
    "@types/tmp": "^0.2.3",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "5.41.0",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^2.16.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.1.0",
    "prettier": "^2.8.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "displayName": "retrieves",
  "engines": {
    "vscode": "^1.74.0"
  },
  "main": "./dist/extension.js",
  "name": "retrieves",
  "publisher": "fluidattacks",
  "scripts": {
    "compile": "webpack",
    "compile-tests": "tsc -p . --outDir out",
    "lint": "eslint . --ext '.ts' --fix",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "watch": "webpack --watch",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "version": "0.4.0"
}
