{
  "activationEvents": [
    "onStartupFinished"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "retrieves.clone",
        "title": "Clone"
      },
      {
        "command": "retrieves.lines",
        "title": "Attacked Lines"
      },
      {
        "command": "retrieves.environmentUrls",
        "title": "Git Environment Urls"
      },
      {
        "command": "retrieves.updateToeLinesAttackedLines",
        "title": "Attacked"
      },
      {
        "command": "retrieves.addSelectedText",
        "title": "Add selected text"
      }
    ],
    "configuration": [
      {
        "properties": {
          "retrieves.api_token": {
            "type": "string"
          },
          "retrieves.useRole": {
            "type": "string",
            "default": "analyst",
            "enum": [
              "analyst"
            ]
          },
          "retrieves.extraGroups": {
            "default": [],
            "description": "add groups that are not listed automatically",
            "items": true,
            "type": "array"
          }
        },
        "title": "Retrieves configuration"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "retrieves.addSelectedText",
          "group": "navigation",
          "when": "editorTextFocus"
        }
      ],
      "editor/title/context": [
        {
          "command": "retrieves.clone",
          "group": "git",
          "when": "config.git.enabled"
        }
      ],
      "explorer/context": [
        {
          "command": "retrieves.clone",
          "group": "git",
          "when": "config.git.enabled && resourcePath =~ **/services/groups/([a-z]*)/fusion/([a-z]*)$/"
        },
        {
          "command": "retrieves.updateToeLinesAttackedLines",
          "group": "navigation",
          "when": "resourcePath =~ **/groups/** && resourceScheme == file"
        }
      ],
      "view/item/context": [
        {
          "command": "retrieves.clone",
          "when": "view == user_groups && viewItem == gitRoot"
        },
        {
          "command": "retrieves.environmentUrls",
          "when": "view == user_groups && viewItem == gitRoot"
        },
        {
          "command": "retrieves.lines",
          "when": "view == user_groups && viewItem == gitRoot"
        }
      ],
      "webview/context": [
        {
          "command": "retrieves.updateToeLinesAttackedLines",
          "when": "webviewSection == 'filename'"
        }
      ]
    },
    "views": {
      "groups_explorer": [
        {
          "contextualTitle": "Retrieves",
          "icon": "media/dep.svg",
          "id": "user_groups",
          "name": "User Groups"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "media/dep.svg",
          "id": "groups_explorer",
          "title": "Groups Manager"
        }
      ]
    }
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@types/js-yaml": "^4.0.5",
    "@types/ramda": "^0.28.22",
    "@types/vscode-webview": "^1.57.1",
    "@vscode/webview-ui-toolkit": "^1.2.1",
    "cross-fetch": "^3.1.5",
    "glob": "^8.1.0",
    "graphql": "^16.6.0",
    "html-creator": "^0.6.2",
    "js-yaml": "^4.1.0",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.8.0",
    "simple-git": "^3.15.1",
    "tar-stream": "^3.0.0",
    "tmp": "^0.2.1"
  },
  "description": "",
  "devDependencies": {
    "@estruyf/vscode": "^1.1.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/tar-stream": "^2.2.2",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "@vscode/vsce": "^2.16.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "displayName": "retrieves",
  "engines": {
    "vscode": "^1.71.0"
  },
  "main": "./dist/extension.js",
  "name": "retrieves",
  "publisher": "fluidattacks",
  "repository": {
    "type": "git",
    "url": "https://github.com/estruyf/vscode-react-webview-template"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "webpack",
    "compile-tests": "tsc -p . --outDir out",
    "lint": "eslint src --ext ts,tsx --fix --format codeframe",
    "package": "npm run clean && npm-run-all --parallel package:*",
    "package:ext": "webpack --mode production --config ./webpack/extension.config.js",
    "package:wv": "webpack --mode production --config ./webpack/webview.config.js",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch-tests": "tsc -p . -w --outDir out",
    "watch:ext": "webpack --mode development --watch --config ./webpack/extension.config.js",
    "watch:wv": "webpack serve --mode development --config ./webpack/webview.config.js"
  },
  "version": "0.17.0"
}
