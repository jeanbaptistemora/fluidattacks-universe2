if test -z "${PRODUCT_DEV_ENV:-}"; then
  echo
  echo "In order to load development environments please pick one from:"
  echo
  echo "  $ export PRODUCT_DEV_ENV=integratesBack"
  echo
  echo "And then run:"
  echo
  echo "  $ direnv allow"
  echo
  echo "Now re-open your code editor and start developing."
  echo
  echo "If you want to load the dev environment automatically"
  echo "consider adding the export at the end of your: ~/.bashrc"
else
  m . "/dev/${PRODUCT_DEV_ENV}" \
    && source "${HOME}/.makes/out-dev-${PRODUCT_DEV_ENV}/template"
fi
