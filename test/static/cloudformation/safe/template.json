          "Statement": [
            {
              "Action": [
                "ecr:Get*"
              ],
              "Effect": "Allow",
              "Resource": [
                "arn:aws:ecr:us-east-1::repository/*"
              ]
            }
          ],
          "Version": "2012-10-17"
        },
        "PolicyName": "policy1",
        "Users": []
