          {
            "PolicyDocument": {
              "Statement": [
                {
                  "Action": "*",
                  "Effect": "Deny",
                  "Resource": "*"
                },
                {
                  "Action": [
                    "ecr:Get*"
                  ],
                  "Effect": "Allow",
                  "Resource": [
