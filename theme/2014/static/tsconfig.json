{
  "compilerOptions": {
    "allowJs": false,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2018"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2019"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "js/*"
  ]
}
