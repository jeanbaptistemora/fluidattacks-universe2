<VirtualHost *:80>
	ServerName integrates.fluid.la
	ServerAdmin engineering@fluid.la

	DocumentRoot /var/www/fluid-integrates/fluidintegrates
	WSGIScriptAlias / /var/www/fluid-integrates/fluidintegrates/apache/wsgi.py
	Alias /assets/ /var/www/fluid-integrates/app/assets/
	<Directory "/var/www/fluid-integrates/fluidintegrates/apache/">
		Require all granted
	</Directory>

        <Directory "/assets/">
                Require all granted
        </Directory>
	
	ErrorLog /var/log/apache2/error.log
	CustomLog /var/log/apache2/custom.log combined
</VirtualHost>
