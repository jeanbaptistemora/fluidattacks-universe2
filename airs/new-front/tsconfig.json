{
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": "src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "es2018",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2019",
  },
  "exclude": [
    "node_modules"
  ],
  "include": ["./src/**/*"]
}
