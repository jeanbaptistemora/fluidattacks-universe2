{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "dockersock",
      "host": {
        "sourcePath": "/var/run/docker.sock"
      }
    },
    {
      "name": "dockerjson",
      "host": {
        "sourcePath": "/root/.docker/config.json"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "integrates",
      "image": "205810638802.dkr.ecr.us-east-1.amazonaws.com/integrates:master",
      "essential": true,
	  "memory": 256,
	  "Update": "true",
	  "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000
        }
      ]
    },
    {
      "name": "alg",
      "image": "205810638802.dkr.ecr.us-east-1.amazonaws.com/serves:alg",
      "essential": true,
	  "memory": 256,
	  "Update": "true",
      "portMappings": [
        {
          "hostPort": 443,
          "containerPort": 443
        },
        {
          "hostPort": 80,
          "containerPort": 80
        }

      ]
    },
    {
      "name": "exams",
      "image": "205810638802.dkr.ecr.us-east-1.amazonaws.com/serves:exams",
      "essential": true,
	  "memory": 256,
	  "Update": "true",
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 80
        }
      ]
    },
    {
      "name": "watchtower",
      "image": "v2tec/watchtower",
      "essential": true,
          "memory": 256,
          "Update": "true",
      "mountPoints": [
        {
          "sourceVolume": "dockersock",
          "containerPath": "/var/run/docker.sock"
        },
        {
          "sourceVolume": "dockerjson",
          "containerPath": "/config.json"
        }
      ]
    }
  ]
}
